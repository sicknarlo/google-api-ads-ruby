source 'http://rubygems.org'

gem 'rails', '4.2.5'
gem 'jquery-rails'
gem 'activerecord-session_store'

# Using sqlite by default.
group :development, :test do
  gem 'sqlite3'
end

group :production do
  gem 'pg'
  gem 'rails_12factor'
end

# AdWords API gem.
gem 'google-adwords-api'
gem 'google-api-client'

# Gem for OAuth authorization.
gem 'oauth2'
gem 'tzinfo-data'
